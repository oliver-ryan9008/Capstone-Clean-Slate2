@model IEnumerable<Capston_Clean_Slate2.Models.Schedule>

@{
    ViewBag.Title = "ViewSchedule";
    Layout = null;
}
<link href="https://stackpath.bootstrapcdn.com/bootswatch/4.1.3/minty/bootstrap.min.css" rel="stylesheet" integrity="sha384-Qt9Hug5NfnQDGMoaQYXN1+PiQvda7poO7/5k4qAmMN6evu0oDFMJTyjqaoTGHdqf" crossorigin="anonymous" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous" />

<head runat="server">
    <title>Index</title>
    <script src="~/Scripts/dhtmlxScheduler/dhtmlxscheduler.js"
            type="text/javascript"></script>
    <link href="~/Scripts/dhtmlxScheduler/dhtmlxscheduler.css"
          rel="stylesheet" type="text/css" />
    <style type="text/css">
        html, body {
            height: 100%;
            padding: 0px;
            margin: 0px;
        }
    </style>
    <script type="text/javascript">
        function init() {
            scheduler.config.xml_date = "%Y-%m-%d %H:%i";
            scheduler.init("scheduler_here", new Date(), "month");
            @*@foreach (var i in Model)
            {
                <text> scheduler.parse([{ id:"@i.Id", start_date:"@i.StartTime", end_date:"@i.EndTime", text:"@i.Text"}], "json");
            scheduler.addEvent(@i.Id);
            </text>
            }*@

        }
    </script>
    <style type="text/css">
        body {
            padding-top: 75px !important;
        }
    </style>
</head>
<body onload="init()">
    <style>
        .navbar {
            margin-bottom: 5px;
        }

        .mr-auto {
        }
    </style>
    <nav class="navbar navbar-expand-sm navbar-dark fixed-top bg-primary d-flex">
        <div class="container">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"></button>
            <div class="navbar-collapse collapse container" id="navbarSupportedContent" style="max-height:50px">
                <a class="navbar-brand">
                    <img src="https://i.imgur.com/6pl2J70.png" alt="BMVLogo" height="45" />
                </a>

                <ul class="navbar-nav mr-auto" style="margin-top: -20px">
                    <li>
                        <a class="nav-link" href="~/Home/Index">
                            <span class="fas fa-home"></span><br />
                            Home
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div id="scheduler_here" class="dhx_cal_container" style='width:100%; height:100%;'>
        <div class="dhx_cal_navline">
            <div class="dhx_cal_prev_button"> </div>
            <div class="dhx_cal_next_button"> </div>
            <div class="dhx_cal_today_button"></div>
            <div class="dhx_cal_date"></div>
            <div class="dhx_cal_tab" name="day_tab" style="removed:204px;"></div>
            <div class="dhx_cal_tab" name="week_tab" style="removed:140px;"></div>
            <div class="dhx_cal_tab" name="month_tab" style="removed:76px;"></div>
        </div>
        <div class="dhx_cal_header">
        </div>
        <div class="dhx_cal_data">
        </div>
    </div>
</body>